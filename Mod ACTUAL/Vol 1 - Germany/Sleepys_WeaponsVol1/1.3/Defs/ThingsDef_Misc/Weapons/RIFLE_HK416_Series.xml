      <AccuracyLong>0.30</AccuracyLong>
      <RangedWeapon_Cooldown>1.55</RangedWeapon_Cooldown>
    </statBases>
	<!--
    <costList>
      <Steel>60</Steel>
      <ComponentIndustrial>8</ComponentIndustrial>
    </costList>
	-->
