  <ThingDef ParentName="BaseHumanMakeableGun">
    <defName>SLP_Gun_AUT_AUGA3SASE_S</defName>
    <label>AUG A3 SASE</label>
    <description>A general-purpose gas-operated assault rifle for field or urban combat. It has good range, decent power, and good accuracy. This particular model has an integrated scope.</description>
    <graphicData>
      <texPath>Things/Item/Equipment/Rifles/SLP_AUT_AUGA3SASE_S</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
	
	<equippedAngleOffset>-45</equippedAngleOffset>
	
    <soundInteract>SLP_AUGInter</soundInteract>
    <weaponTags>
      <li>IndustrialGunAdvanced</li>
      <li>AssaultRifle</li>
    </weaponTags>

    <recipeMaker>
      <researchPrerequisite>SLP_AdvSM</researchPrerequisite>
      <skillRequirements>
        <Crafting>6</Crafting>
      </skillRequirements>
    </recipeMaker>
    <thingSetMakerTags><li>RewardStandardQualitySuper</li></thingSetMakerTags>
    <statBases>
	  <MarketValue>453</MarketValue>
      <WorkToMake>40000</WorkToMake>
      <Mass>3.6</Mass>
      <AccuracyTouch>0.63</AccuracyTouch>
      <AccuracyShort>0.75</AccuracyShort>
      <AccuracyMedium>0.71</AccuracyMedium>
      <AccuracyLong>0.57</AccuracyLong>
      <RangedWeapon_Cooldown>1.79</RangedWeapon_Cooldown>
    </statBases>
    <costList>
      <Steel>60</Steel>
      <ComponentIndustrial>7</ComponentIndustrial>
    </costList>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>Bullet_SLP_AUGA3SASE_S</defaultProjectile>
        <warmupTime>1.22</warmupTime>
        <range>32.9</range>
		<burstShotCount>3</burstShotCount>
        <ticksBetweenBurstShots>10</ticksBetweenBurstShots> <!-- 1200RPM=3, 900RPM=4, 720RPM=5, 600RPM=6, 512.29RPM=7, 450RPM=8, 400RPM=9, 360RPM=10, 327.27RPM=11, 300RPM=12, 240RPM=15-->
        <soundCast>SLP_RIFLE_AUGShot2</soundCast>
        <soundCastTail>GunTail_Medium</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
    <tools>
      <li>
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
  </ThingDef>
  
