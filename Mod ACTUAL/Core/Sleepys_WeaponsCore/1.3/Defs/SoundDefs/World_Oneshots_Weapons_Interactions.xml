					<max>1.137647</max>
				</pitchRange>
				<distRange>
					<min>0</min>
					<max>50.40025</max>
				</distRange>
				<!--<sustainLoop>False</sustainLoop>-->
				<tempoAffectedByGameSpeed>true</tempoAffectedByGameSpeed>
			</li>
		</subSounds>
	</SoundDef>
	
